<%= render 'layouts/navbar' %>

<section id = "resume" class = "resume section offset-header">
	<div class = "container text-center">
		<h1> All the resumes are here</h1>
		<div class = "row" %>
			<% @resumes.each do |r|%>
				<div class = "col-md-4 col-xs-12 col-lg-3" %>
					
					<%= link_to image_tag(get_image_url(r.id, r.resume_file_name), class: "resume-thumbnail"), resume_path(r) %>
				</div>
			<%end %>
		</div>
	</div>
</section>

