
<section id = "resume" class = "resume section offset-header">
	<div class = "container text-center">
		<% provide(:title, 'Add Resume') %>
		<h1>Add Resume</h1>
		<div class="row">
			<div class="col-md-6 col-md-offset-3">
				<%= form_for(@resume) do |f| %>
				<%= render 'shared/error_messages' %>

				<%= f.label :url %>
				<%= f.text_field :url, class: 'form-control' %>

				<%= f.submit "Create resume", class: "btn btn-primary" %>
		  	</div>
		</div>
	</div>
