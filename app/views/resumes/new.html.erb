<% provide(:title, 'Add Resume') %>
<h1>Add Resume</h1>

<div class="row">
	<div class="col-md-6 col-md-offset-3">
		<%= form_for(@resume) do |f| %>
      		<%= f.label :url %>
      		<%= f.text_field :url %>

      		<%= f.submit "Create resume", class: "btn btn-primary" %>
    	<% end %>
  	</div>
</div>